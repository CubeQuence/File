{
    "name": "cubequence/file",
    "description": "PHP handle files and folders",
    "keywords": [
        "file",
        "cubequence"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Castelnuovo",
            "email": "luca@castelnuovo.xyz"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "cubequence/crypto": "^1.0",
        "league/flysystem": "^2.0",
        "league/flysystem-aws-s3-v3": "^2.0"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "dev-master",
        "pestphp/pest": "^1.0"
    },
    "scripts": {
        "tests": "./vendor/bin/pest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CQ\\File\\": "src/File"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/CubeQuence/File/issues",
        "source": "https://github.com/CubeQuence/File"
    }
}
